public interface IDelegadoNegocios {
	
	public List<UsuarioDTO> getTodosUsuarios() throws Exception;	
	
	public List<UsuarioDTO> getUsuarioPorId() throws Exception;
	
	public List<UsuarioDTO> eliminarUsuario() throws Exception;
	
	public List<UsuarioDTO> insertarUsuario() throws Exception;

}
