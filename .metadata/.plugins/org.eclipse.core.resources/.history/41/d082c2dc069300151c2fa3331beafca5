package co.gov.dane.framework.cache;

import net.sf.ehcache.CacheManager;

public class Cache {
	
	private static Cache instancia = null;
	private CacheManager cm;
	
	private Cache(){}
	
	public static Cache getInstancia(){
		
		if(instancia == null){
			instancia = new Cache();
			
		}
		
		return instancia;
		
	}
	
	
	
	

}
