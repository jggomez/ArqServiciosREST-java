package co.gov.dane.framework.cache;

import net.sf.ehcache.CacheManager;

public class Cache {
	
	private static Cache instancia = null;
	private static CacheManager cm;
	private static net.sf.ehcache.Cache = null;
	
	private Cache(){}
	
	public static Cache getInstancia(){
		
		if(instancia == null){
			instancia = new Cache();
			cm = CacheManager.newInstance();
			cache = cm.getCache("cacheconf");
		}
		
		return instancia;
		
	}
	
	
	
	

}
